#### Figure 2
```{r include=FALSE}
scaleFUN <- function(x) sprintf("%.1f", x)
p1_Pace = tidy(gee_Pace_2_female) %>% 
  cbind(as_tibble(confint.lm(gee_Pace_2_female))) %>% 
  filter(term == "dwk_neg1vsall_new2" | term == "dwk0vsall_new2" | term == "dwk1vsall_new2" | term == "dwk2vsall_new2" | term == "dwk3vsall_new2" | term == "dwk4vsall_new2")
p1_Pace = tidy(gee_Pace_2_male) %>% 
  cbind(as_tibble(confint.lm(gee_Pace_2_male))) %>% 
  filter(term == "dwk_neg1vsall_new2" | term == "dwk0vsall_new2" | term == "dwk1vsall_new2" | term == "dwk2vsall_new2" | term == "dwk3vsall_new2" | term == "dwk4vsall_new2") %>% 
  rbind(p1_Pace)
p1_Pace$name <- c("Men", "Men", "Men", "Men", "Men", "Men",
                  "Women", "Women", "Women", "Women", "Women", "Women") 
p1_Pace$name <- factor(p1_Pace$name, levels = c("Women", "Men"))
p1_Pace$dwk <- c("dwk-1", "dwk0", "dwk1", "dwk2", "dwk3", "dwk4")
p1_Pace$title <- "DunedinPACE"
f1_Pace <- ggplot(p1_Pace, aes(dwk, estimate, color = name, group = name)) + theme_bw() +
        facet_grid(. ~ title)
f1_Pace <- f1_Pace +        
  geom_point(size = 5, aes(color = name, shape = name)) +
    labs(
    x = "Timing at Famine Exposure",
    y = "Effect Size",
    colour = "") +
  geom_line(size = 1.2) +
  scale_x_discrete(limits = rev) +
  scale_y_continuous(limits = c(-0.45, 0.45), breaks = seq(-0.4, 0.4, 0.2), labels=scaleFUN) +
  geom_hline(aes(yintercept = 0.0), color = "#666666", linewidth = 1, linetype="dashed") +
  theme(strip.text.x = element_text(size = 25),
        strip.background = element_rect(color = "black", size = 1.2),
        legend.title = element_blank(),
        legend.position = "top",
        legend.text = element_text(size = 25, color = "black"),
        axis.text = element_text(size = 25, color = "black"),
        panel.grid.major = element_line(linetype = "dotted", color = "grey", size = 0.8),
        panel.grid.minor = element_blank(),
        panel.border = element_blank(),
        axis.line.y.left = element_line(colour='grey', linewidth = 1),
        axis.line.x.bottom = element_line(colour='grey', linewidth = 1),
        axis.ticks = element_line(colour='grey', linewidth = 1),
        axis.ticks.length=unit(.25, "cm")) +
        scale_color_manual(values = c("navyblue", "navyblue")) +
  annotate("rect", xmin = "dwk4", xmax = "dwk0", ymin = -Inf, ymax = Inf, alpha = .1) + guides(shape=guide_legend("name"), color="none")


pc_p1_GrimAge = tidy(gee_pc_GrimAge_2_female) %>% 
  cbind(as_tibble(confint.lm(gee_pc_GrimAge_2_female))) %>% 
  filter(term == "dwk_neg1vsall_new2" | term == "dwk0vsall_new2" | term == "dwk1vsall_new2" | term == "dwk2vsall_new2" | term == "dwk3vsall_new2" | term == "dwk4vsall_new2")
pc_p1_GrimAge = tidy(gee_pc_GrimAge_2_male) %>% 
  cbind(as_tibble(confint.lm(gee_pc_GrimAge_2_male))) %>% 
  filter(term == "dwk_neg1vsall_new2" | term == "dwk0vsall_new2" | term == "dwk1vsall_new2" | term == "dwk2vsall_new2" | term == "dwk3vsall_new2" | term == "dwk4vsall_new2") %>% 
  rbind(pc_p1_GrimAge)
pc_p1_GrimAge$name <- c("Men", "Men", "Men", "Men", "Men", "Men",
                       "Women", "Women", "Women", "Women", "Women", "Women") 
pc_p1_GrimAge$name <- factor(pc_p1_GrimAge$name, levels = c("Women", "Men"))
pc_p1_GrimAge$dwk <- c("D-1", "D0", "D1", "D2", "D3", "D4")
pc_p1_GrimAge$title <- "PC PhenoAge"
pc_f1_GrimAge <- ggplot(pc_p1_GrimAge, aes(dwk, estimate, color = name, group = name)) + theme_bw() +
        facet_grid(. ~ title)
pc_f1_GrimAge <- pc_f1_GrimAge +        
  geom_point(size = 5, aes(color = name, shape = name)) +
    labs(
    x = "Timing at Famine Exposure",
    y = "Effect Size",
    colour = "") +
  geom_line(size = 1.2) +
  scale_x_discrete(limits = rev) +
  scale_y_continuous(limits = c(-0.45, 0.45), breaks = seq(-0.4, 0.4, 0.2), labels=scaleFUN) +
  geom_hline(aes(yintercept = 0.0), color = "#666666", linewidth = 1, linetype="dashed") +
  theme(strip.text.x = element_text(size = 25),
        strip.background = element_rect(color = "black", size = 1.2),
        legend.title = element_blank(),
        legend.position = "top",
        legend.text = element_text(size = 25, color = "black"),
        axis.text = element_text(size = 25, color = "black"),
        panel.grid.major = element_line(linetype = "dotted", color = "grey", size = 0.8),
        panel.grid.minor = element_blank(),
        panel.border = element_blank(),
        axis.line.y.left = element_line(colour='grey', linewidth = 1),
        axis.line.x.bottom = element_line(colour='grey', linewidth = 1),
        axis.ticks = element_line(colour='grey', linewidth = 1),
        axis.ticks.length=unit(.25, "cm")) +
        scale_color_manual(values = c("darkorange2", "darkorange2")) +
  annotate("rect", xmin = "D4", xmax = "D0", ymin = -Inf, ymax = Inf, alpha = .1) + guides(shape=guide_legend("name"), color="none")


pc_p1_Levine = tidy(gee_pc_Levine_2_female) %>% 
  cbind(as_tibble(confint.lm(gee_pc_Levine_2_female))) %>% 
  filter(term == "dwk_neg1vsall_new2" | term == "dwk0vsall_new2" | term == "dwk1vsall_new2" | term == "dwk2vsall_new2" | term == "dwk3vsall_new2" | term == "dwk4vsall_new2")
pc_p1_Levine = tidy(gee_pc_Levine_2_male) %>% 
  cbind(as_tibble(confint.lm(gee_pc_Levine_2_male))) %>% 
  filter(term == "dwk_neg1vsall_new2" | term == "dwk0vsall_new2" | term == "dwk1vsall_new2" | term == "dwk2vsall_new2" | term == "dwk3vsall_new2" | term == "dwk4vsall_new2") %>% 
  rbind(pc_p1_Levine)
pc_p1_Levine$name <- c("Men", "Men", "Men", "Men", "Men", "Men",
                        "Women", "Women", "Women", "Women", "Women", "Women") 
pc_p1_Levine$name <- factor(pc_p1_Levine$name, levels = c("Women", "Men"))
pc_p1_Levine$dwk <- c("dwk-1", "dwk0", "dwk1", "dwk2", "dwk3", "dwk4")
pc_p1_Levine$title <- "PC Levine"
pc_f1_Levine <- ggplot(pc_p1_Levine, aes(dwk, estimate, color = name, group = name)) + theme_bw() +
        facet_grid(. ~ title)
pc_f1_Levine <- pc_f1_Levine +        
  geom_point(size = 5, aes(color = name, shape = name)) +
    labs(
    x = "Timing at Famine Exposure",
    y = "Effect Size",
    colour = "") +
  geom_line(size = 1.2) +
  scale_x_discrete(limits = rev) +
  scale_y_continuous(limits = c(-0.45, 0.45), breaks = seq(-0.4, 0.4, 0.2), labels=scaleFUN) +
  geom_hline(aes(yintercept = 0.0), color = "#666666", linewidth = 1, linetype="dashed") +
  theme(strip.text.x = element_text(size = 25),
        strip.background = element_rect(color = "black", size = 1.2),
        legend.title = element_blank(),
        legend.position = "top",
        legend.text = element_text(size = 25, color = "black"),
        axis.text = element_text(size = 25, color = "black"),
        panel.grid.major = element_line(linetype = "dotted", color = "grey", size = 0.8),
        panel.grid.minor = element_blank(),
        panel.border = element_blank(),
        axis.line.y.left = element_line(colour='grey', linewidth = 1),
        axis.line.x.bottom = element_line(colour='grey', linewidth = 1),
        axis.ticks = element_line(colour='grey', linewidth = 1),
        axis.ticks.length=unit(.25, "cm")) +
        scale_color_manual(values = c("#990000", "#990000")) +
  annotate("rect", xmin = "dwk4", xmax = "dwk0", ymin = -Inf, ymax = Inf, alpha = .08) + guides(shape=guide_legend("name"), color="none")
        

### into panel plot
remove_y <- theme(
  axis.text.y = element_blank(),
  axis.ticks.y = element_blank(),
  axis.title.y = element_blank(),
  axis.line.y.left = element_blank())

remove_x <- theme(
  axis.text.x = element_blank(),
  axis.ticks.x = element_blank(),
  axis.title.x = element_blank(), 
  axis.line.x.bottom = element_blank())


Figure_2 <- ggarrange(f1_Pace + rremove("ylab") + remove_x,
                             pc_f1_GrimAge + rremove("ylab") + remove_x,
                             pc_f1_Levine + rremove("ylab") + rremove("xlab"),
                             ncol = 1, nrow = 3, legend.grob = get_legend(pc_f1_Levine), legend = "top")
Figure_2 <- annotate_figure(Figure_2,
                left = text_grob("Effect size", rot = 90, size = 30, face = "bold", vjust = -0.5), 
                bottom = text_grob("Gestational time windows at famine exposure", size = 30, face = "bold", vjust = 1.5)) +
                theme(plot.margin = unit(c(1,1,1,1), "cm"))
