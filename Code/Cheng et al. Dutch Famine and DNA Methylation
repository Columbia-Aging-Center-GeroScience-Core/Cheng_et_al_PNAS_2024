tbl_model1_tblS2 <- 
        tbl_stack(list(gee_Pace_1_t, gee_Pace_1_female_t, gee_Pace_1_male_t, gee_Pace_1_interact_t, 
                       gee_pc_GrimAge_1_t, gee_pc_GrimAge_1_female_t, gee_pc_GrimAge_1_male_t, gee_pc_GrimAge_1_interact_t, 
                       gee_pc_Levine_1_t, gee_pc_Levine_1_female_t, gee_pc_Levine_1_male_t, gee_pc_Levine_1_interact_t), quiet = TRUE) %>% 
              as_gt() %>%
              gt::tab_style(
                style = gt::cell_text(weight = "bold"),
                locations = gt::cells_row_groups(groups = everything())
              ) %>% 
              gt::tab_header(title = "Effect of famine exposure")
