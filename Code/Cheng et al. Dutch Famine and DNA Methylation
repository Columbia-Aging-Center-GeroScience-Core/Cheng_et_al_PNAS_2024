## Import Data
```{r include=FALSE}
sample_sheet <- read_tsv(here::here("Data", "samplesheet_complete_update230222.tsv"))
load(here::here("Data", "DHWFS_backbone.RData"))
load(here::here("Data", "DHWFS_exposure_20221215.RData"))
DHWFS_exposure <- DHWFS_exposure_20221215
new_backbone <- read.csv(here::here("Data", "DHWFS_backbone_wFTCsample.csv"))
clocks <- read.dta13(here::here("Data", "CPRDNAmClocks.dta"), convert.factors = F, generate.factors = F)
clock_zhang <- read.dta13(here::here("Data", "CPRDNAmClocks_wZhang.dta"), convert.factors = F, generate.factors = F)
bmi_data <- read.csv(here::here("Data", "DHWFS_merged_data_1028.csv"))
clocks_std <- read.csv(here::here("Data", "all_clocks_std.csv"))
clocks_std_includingZhang <- read.csv(here::here("Data", "all_clocks_std_includingZhang.csv"))
anthro_data <- read.dta13(here::here("Data", "anthro.dta"), convert.factors = F, generate.factors = F)
srh_data <- read.dta13(here::here("Data", "sf35a.dta"), convert.factors = F, generate.factors = F)
```


## Merge Data & Data Management (Telephone Sample N=1031)
```{r include=FALSE}
## Rename/Transform Variables
sample_sheet <- sample_sheet %>%
        rename(sample_id = sentrix)
DHWFS_backbone <- DHWFS_backbone %>%
        rename(hofnum = hofnum1,
               Age = agelab) 
DHWFS_backbone$exp_10wks = DHWFS_backbone$prenatal_exp_weeks/10
DHWFS_exposure <- DHWFS_exposure %>%
        rename(hofnum = hofnum1)
new_backbone <- new_backbone %>%
        rename(hofnum = hofnum1)
clocks <- clocks %>%
        rename(sample_id = sentrix,
               Pace = dunedinpace,
               GrimAge = dnamgrimage,
               Levine = dnamphenoage,
               Horvath = dnamage,
               Horvath2 = dnamageskinblood,
               pc_GrimAge = pcgrimage,
               pc_Levine = pcphenoage,
               pc_Horvath = pchorvath1,
               pc_Horvath2= pcskinblood)
clock_zhang <- clock_zhang %>%
        rename(Zhang = zhangclock)
bmi_data <- bmi_data %>%
        rename(hofnum = hofnum1)
anthro_data <- anthro_data %>%
        rename(hofnum = hofnum1)
srh_data <- srh_data %>%
        rename(hofnum = hofnum1)

## Variables of Interest
sample_sheet <- sample_sheet %>%
        select(hofnum, sample_id,sample_plate, column, array)
DHWFS_backbone <- DHWFS_backbone %>%
        select(hofnum, idfam, subtype, sibs, sex, Age, catthree3, anywk_new2, bproxdate, almpdate, prenatal_exp_weeks, exp_10wks)
DHWFS_exposure <- DHWFS_exposure %>%
        select(hofnum, dwk_neg1vsall_new2, dwk0vsall_new2, dwk1vsall_new2, dwk2vsall_new2, dwk3vsall_new2, dwk4vsall_new2)
new_backbone <- new_backbone %>%
        select(hofnum, FTCsample, famexp)
clocks <- clocks %>%
        select(hofnum,
               bcell, cd4t, cd8t, gran, mono, nk,
               Pace, GrimAge, Levine, Horvath, Horvath2, 
               pc_GrimAge, pc_Levine, pc_Horvath, pc_Horvath2) 
clock_zhang <- clock_zhang %>%
        select(hofnum, Zhang)
bmi_data <- bmi_data %>%
        select(hofnum, bmi)
anthro_data <- anthro_data %>%
        select(hofnum, sbp, dbp)
clocks_std_includingZhang <- clocks_std_includingZhang %>%
        select(hofnum, Zhang_std)

## Merge Data
dhwfs_data <- merge(sample_sheet, DHWFS_backbone, by = "hofnum", all = TRUE) 
dhwfs_data <- merge(dhwfs_data, DHWFS_exposure, by = "hofnum", all = TRUE) 
dhwfs_data <- merge(dhwfs_data, new_backbone, by = "hofnum", all = TRUE) 
dhwfs_data <- merge(dhwfs_data, clocks, by = "hofnum", all = TRUE) 
dhwfs_data <- merge(dhwfs_data, clock_zhang, by = "hofnum", all = TRUE) 
dhwfs_data <- merge(dhwfs_data, bmi_data, by = "hofnum", all = TRUE) 
dhwfs_data <- merge(dhwfs_data, anthro_data, by = "hofnum", all = TRUE)
dhwfs_data <- merge(dhwfs_data, srh_data, by = "hofnum", all = TRUE)

## Data Management 
dhwfs_data <- dhwfs_data %>% 
  mutate(Sex = case_when(sex == 1 ~ 1,
                         sex == 2 ~ 0)) #originally: 1=male, 2=female; transformed: 0=female, 1=male
dhwfs_data$Sex <- as.factor(dhwfs_data$Sex)
dhwfs_data$catthree3 <- as.factor(dhwfs_data$catthree3)
dhwfs_data$sample_plate <- as.factor(dhwfs_data$sample_plate)
dhwfs_data$column <- as.factor(dhwfs_data$column)
dhwfs_data$array <- as.factor(dhwfs_data$array)
dhwfs_data$Agesq <- dhwfs_data$Age^2

## Code hypertension based on the current guidelines as SBP>=130 | DBP>=80
dhwfs_data <- dhwfs_data %>% 
  mutate(hypertension = case_when(!(sbp < 130) | !(dbp < 80) ~ 1,
                                  sbp < 130 | dbp < 80 ~ 0))
```


## Sample N
Full sample (n=951)
```{r include=FALSE}
### DHWFS_data
DHWFS_data <- dhwfs_data %>% 
        drop_na(Age, Sex, hofnum, Pace, GrimAge, Levine, Horvath, Horvath2, pc_GrimAge, pc_Levine, pc_Horvath, pc_Horvath2)

## LMP_Date
DHWFS_data$LMP_Date <- as.Date(DHWFS_data$almpdate)

## prenatal_exp_10wks
DHWFS_data <- DHWFS_data %>%
        mutate(prenatal_exp_10wks = case_when(anywk_new2==0 ~ 0,
                                              is.na(exp_10wks) ~ 0,
                                              !is.na(exp_10wks) ~ exp_10wks))
## Merge with std data
DHWFS_data <- merge(DHWFS_data, clocks_std, by = "hofnum")

## Female (n=521)
DHWFS_data_female <- DHWFS_data %>% 
  filter(Sex == 0)

## Male (n=430)
DHWFS_data_male <- DHWFS_data %>% 
  filter(Sex == 1)
```


Restricted to famine-exposed and time controls (n=646: 487 famine exposed + 159 time controls)
```{r include=FALSE}
DHWFS_data_tc <- DHWFS_data %>%
        filter(!(catthree3 == 2)) 
DHWFS_data_tc$birthdate <- as.Date(DHWFS_data_tc$bproxdate)
DHWFS_data_tc_female <- DHWFS_data_tc %>%
        filter(Sex == 0) # 345 female
DHWFS_data_tc_male <- DHWFS_data_tc %>%
        filter(Sex == 1) # 301 male
```


Sibling sample (n=454, 227 pairs)
```{r include=FALSE}
### DHWFS_data
DHWFS_data_sib <- DHWFS_data %>%
        filter(famexp == 1)
idfam_DHWFS_data_sib <- DHWFS_data_sib %>%
        group_by(idfam) %>%
        summarise(n = n()) %>%
        filter(n == 2) %>%
        pull(idfam) #227 pairs
DHWFS_data_sib <- DHWFS_data_sib %>%
        filter(idfam %in% idfam_DHWFS_data_sib) #454

## LMP_Date
DHWFS_data_sib$LMP_Date <- as.Date(DHWFS_data_sib$almpdate)

## Female (n=258)
DHWFS_data_sib_female = DHWFS_data_sib %>% 
  filter(Sex == 0)

## Male (n=196)
DHWFS_data_sib_male = DHWFS_data_sib %>% 
  filter(Sex == 1)
```



## Descriptive Analysis: Table 1
```{r warning=FALSE, include=FALSE}
DHWFS_data$dwk4vsall_new2 <- as.factor(DHWFS_data$dwk4vsall_new2)
DHWFS_data$dwk3vsall_new2 <- as.factor(DHWFS_data$dwk3vsall_new2)
DHWFS_data$dwk2vsall_new2 <- as.factor(DHWFS_data$dwk2vsall_new2)
DHWFS_data$dwk1vsall_new2 <- as.factor(DHWFS_data$dwk1vsall_new2)
DHWFS_data$dwk0vsall_new2 <- as.factor(DHWFS_data$dwk0vsall_new2)
DHWFS_data$dwk_neg1vsall_new2 <- as.factor(DHWFS_data$dwk_neg1vsall_new2)
```

```{r warning=FALSE, include=FALSE}
DHWFS_data %>%
        CreateTableOne(
        vars = select(DHWFS_data, 
                      Age, Sex, prenatal_exp_weeks,
                      Pace, pc_GrimAge, pc_Levine) %>% names(),
        strata = c("catthree3"),
        data = .,
        addOverall = TRUE) -> Table_1
```

```{r}
print(Table_1, varLabels = TRUE)
```

```{r warning=FALSE, include=FALSE}
#Table S1 Panel II
dhwfs_data$dwk4vsall_new2 <- as.factor(dhwfs_data$dwk4vsall_new2)
dhwfs_data$dwk3vsall_new2 <- as.factor(dhwfs_data$dwk3vsall_new2)
dhwfs_data$dwk2vsall_new2 <- as.factor(dhwfs_data$dwk2vsall_new2)
dhwfs_data$dwk1vsall_new2 <- as.factor(dhwfs_data$dwk1vsall_new2)
dhwfs_data$dwk0vsall_new2 <- as.factor(dhwfs_data$dwk0vsall_new2)
dhwfs_data$dwk_neg1vsall_new2 <- as.factor(dhwfs_data$dwk_neg1vsall_new2)
```


## Analysis
#### Models
```{r}
########## Pace ##########
## Pace ~ Famine exposure (full DHWFS: all, female, male, sex difference)
gee_Pace_1 <- geeglm(formula = Pace_std ~ anywk_new2 + Sex + Age + Agesq,
                     data = DHWFS_data,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Pace_1_female <- geeglm(formula = Pace_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_1_male <- geeglm(formula = Pace_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_1_interact <- geeglm(formula = Pace_std ~ anywk_new2*Sex + Age + Agesq,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## Pace ~ Famine exposure (restricted to time controls: all, female, male, sex difference)
gee_Pace_1_tc <- geeglm(formula = Pace_std ~ anywk_new2 + Sex,
                     data = DHWFS_data_tc,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Pace_1_tc_female <- geeglm(formula = Pace_std ~ anywk_new2,
                     data = DHWFS_data_tc_female,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Pace_1_tc_male <- geeglm(formula = Pace_std ~ anywk_new2,
                     data = DHWFS_data_tc_male,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Pace_1_tc_interact <- geeglm(formula = Pace_std ~ anywk_new2*Sex,
                    data = DHWFS_data_tc,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## Pace ~ Famine exposure (siblings sample: all, female, male, sex difference)
fe_Pace_1 <- plm(Pace_std ~ anywk_new2 + Age + Agesq,
                 data = DHWFS_data_sib,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")
fe_Pace_1_female <- plm(Pace_std ~ anywk_new2 + Age + Agesq,
                 data = DHWFS_data_sib_female,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")
fe_Pace_1_male <- plm(Pace_std ~ anywk_new2 + Age + Agesq,
                 data = DHWFS_data_sib_male,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")
fe_Pace_1_interact <- plm(Pace_std ~ anywk_new2*Sex + Age + Agesq,
                 data = DHWFS_data_sib,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")

## Dose-Response (all, female, male, sex difference)
gee_Pace_5.1 <- geeglm(formula = Pace_std ~ prenatal_exp_10wks + Sex + Age + Agesq,
                       data = DHWFS_data,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_Pace_5.1_female <- geeglm(formula = Pace_std ~ prenatal_exp_10wks + Age + Agesq,
                       data = DHWFS_data_female,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_Pace_5.1_male <- geeglm(formula = Pace_std ~ prenatal_exp_10wks + Age + Agesq,
                       data = DHWFS_data_male,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_Pace_5.1_interact <- geeglm(formula = Pace_std ~ prenatal_exp_10wks*Sex + Age + Agesq,
                       data = DHWFS_data,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")

## Pace ~ Exposure in a Specific Period (all, female, male)
gee_Pace_2 <- geeglm(formula = Pace_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Sex + Age + Agesq,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_2_female <- geeglm(formula = Pace_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_2_male <- geeglm(formula = Pace_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")


########## PC GrimAge ##########
## pc_GrimAge ~ Famine exposure (full DHWFS: all, female, male, sex difference)
gee_pc_GrimAge_1 <- geeglm(formula = pc_GrimAge_std ~ anywk_new2 + Sex + Age + Agesq,
                           data = DHWFS_data,
                           family  = gaussian(link = "identity"),
                           id = idfam,
                           corstr = "exchangeable")
gee_pc_GrimAge_1_female <- geeglm(formula = pc_GrimAge_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_pc_GrimAge_1_male <- geeglm(formula = pc_GrimAge_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_pc_GrimAge_1_interact <- geeglm(formula = pc_GrimAge_std ~ anywk_new2*Sex + Age + Agesq,
                                      data = DHWFS_data,
                                      family = gaussian(link = "identity"),
                                      id = idfam,
                                      corstr = "exchangeable")

## pc_GrimAge ~ Famine exposure (restricted to time controls: all, female, male, sex difference)
gee_pc_GrimAge_1_tc <- geeglm(formula = pc_GrimAge_std ~ anywk_new2 + Sex,
                     data = DHWFS_data_tc,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_pc_GrimAge_1_tc_female <- geeglm(formula = pc_GrimAge_std ~ anywk_new2,
                     data = DHWFS_data_tc_female,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_pc_GrimAge_1_tc_male <- geeglm(formula = pc_GrimAge_std ~ anywk_new2,
                     data = DHWFS_data_tc_male,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_pc_GrimAge_1_tc_interact <- geeglm(formula = pc_GrimAge_std ~ anywk_new2*Sex,
                    data = DHWFS_data_tc,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## pc_GrimAge ~ Famine exposure (siblings sample: all, female, male, sex difference)
fe_pc_GrimAge_1 <- plm(pc_GrimAge_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_sib,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_pc_GrimAge_1_female <- plm(pc_GrimAge_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_sib_female,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_pc_GrimAge_1_male <- plm(pc_GrimAge_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_sib_male,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_pc_GrimAge_1_interact <- plm(pc_GrimAge_std ~ anywk_new2*Sex + Age + Agesq,
                 data = DHWFS_data_sib,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")

## Dose-Response (all, female, male, sex difference)
gee_pc_GrimAge_5.1 <- geeglm(formula = pc_GrimAge_std ~ prenatal_exp_10wks + Sex + Age + Agesq,
                             data = DHWFS_data,
                             family = gaussian(link = "identity"),
                             id = idfam,
                             corstr = "exchangeable")
gee_pc_GrimAge_5.1_female <- geeglm(formula = pc_GrimAge_std ~ prenatal_exp_10wks + Age + Agesq,
                             data = DHWFS_data_female,
                             family = gaussian(link = "identity"),
                             id = idfam,
                             corstr = "exchangeable")
gee_pc_GrimAge_5.1_male <- geeglm(formula = pc_GrimAge_std ~ prenatal_exp_10wks + Age + Agesq,
                             data = DHWFS_data_male,
                             family = gaussian(link = "identity"),
                             id = idfam,
                             corstr = "exchangeable")
gee_pc_GrimAge_5.1_interact <- geeglm(formula = pc_GrimAge_std ~ prenatal_exp_10wks*Sex + Age + Agesq,
                       data = DHWFS_data,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")

## pc_GrimAge ~ Exposure in a Specific Period (all, female, male)
gee_pc_GrimAge_2 <- geeglm(formula = pc_GrimAge_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Sex + Age + Agesq,
                       data = DHWFS_data,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_pc_GrimAge_2_female <- geeglm(formula = pc_GrimAge_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_pc_GrimAge_2_male <- geeglm(formula = pc_GrimAge_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")


########## GrimAge ##########
## GrimAge ~ Famine exposure (full DHWFS: all, female, male, sex difference)
gee_GrimAge_1 <- geeglm(formula = GrimAge_std ~ anywk_new2 + Sex + Age + Agesq,
                           data = DHWFS_data,
                           family  = gaussian(link = "identity"),
                           id = idfam,
                           corstr = "exchangeable")
gee_GrimAge_1_female <- geeglm(formula = GrimAge_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_GrimAge_1_male <- geeglm(formula = GrimAge_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_GrimAge_1_interact <- geeglm(formula = GrimAge_std ~ anywk_new2*Sex + Age + Agesq,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## GrimAge ~ Famine exposure (restricted to time controls: all, female, male, sex difference)
gee_GrimAge_1_tc <- geeglm(formula = GrimAge_std ~ anywk_new2 + Sex,
                     data = DHWFS_data_tc,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_GrimAge_1_tc_female <- geeglm(formula = GrimAge_std ~ anywk_new2,
                     data = DHWFS_data_tc_female,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_GrimAge_1_tc_male <- geeglm(formula = GrimAge_std ~ anywk_new2,
                     data = DHWFS_data_tc_male,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_GrimAge_1_tc_interact <- geeglm(formula = GrimAge_std ~ anywk_new2*Sex,
                    data = DHWFS_data_tc,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## GrimAge ~ Famine exposure (siblings sample: all, female, male, sex difference)
fe_GrimAge_1 <- plm(GrimAge_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_sib,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_GrimAge_1_female <- plm(GrimAge_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_sib_female,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_GrimAge_1_male <- plm(GrimAge_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_sib_male,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_GrimAge_1_interact <- plm(GrimAge_std ~ anywk_new2*Sex + Age + Agesq,
                 data = DHWFS_data_sib,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")

## Dose-Response (all, female, male, sex difference)
gee_GrimAge_5.1 <- geeglm(formula = GrimAge_std ~ prenatal_exp_10wks + Sex + Age + Agesq,
                       data = DHWFS_data,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
