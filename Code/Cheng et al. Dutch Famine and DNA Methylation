gee_Pace_1.3_t = tbl_regression(gee_Pace_1.3, estimate_fun = function(x)style_ratio(x, digits = 2), pvalue_fun = function(x) style_pvalue(x, digits = 3), include = "anywk_new2", label = list(anywk_new2 = "DunedinPACE")) %>% bold_p() %>% modify_header(label ~ "**Outcome**") %>% add_n(location = "label") 
