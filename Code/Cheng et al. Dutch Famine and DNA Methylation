```{r warning=FALSE, include=FALSE}
tbl_model1_tblS3_tc <- 
        tbl_stack(list(gee_Pace_1_tc_t, gee_Pace_1_tc_female_t, gee_Pace_1_tc_male_t, gee_Pace_1_tc_interact_t,
                       gee_Horvath_1_tc_t, gee_Horvath_1_tc_female_t, gee_Horvath_1_tc_male_t, gee_Horvath_1_tc_interact_t,
                       gee_pc_Horvath_1_tc_t, gee_pc_Horvath_1_tc_female_t, gee_pc_Horvath_1_tc_male_t, gee_pc_Horvath_1_tc_interact_t,
