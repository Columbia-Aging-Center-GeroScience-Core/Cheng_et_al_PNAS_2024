## Sensitivity Analysis: self-rated physical health as outcome
```{r}
########## SRH: Physical Component Summary ##########
## PCS ~ Famine exposure (full DHWFS: all, female, male, sex difference)
gee_pcs_1 <- geeglm(formula = PCS ~ anywk_new2 + Sex + Age + Agesq,
                     data = DHWFS_data,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_pcs_1_female <- geeglm(formula = PCS ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_pcs_1_male <- geeglm(formula = PCS ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_pcs_1_interact <- geeglm(formula = PCS ~ anywk_new2*Sex + Age + Agesq,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
```

## Results
#### Figure 1
```{r include=FALSE}
## Pace
sex_p1_Pace = tidy(gee_Pace_1_female) %>% 
  cbind(as_tibble(confint.lm(gee_Pace_1_female))) %>% 
  filter(term == "anywk_new2")
sex_p1_Pace = tidy(gee_Pace_1_male) %>% 
  cbind(as_tibble(confint.lm(gee_Pace_1_male))) %>% 
  filter(term == "anywk_new2") %>% 
  rbind(sex_p1_Pace)
sex_p1_Pace$name <- c("3_Pace_Men", "1_Pace_Women") 
sex_p1_Pace$title <- "DunedinPACE"
sex_p1_Pace$lower_ci <- sex_p1_Pace$`2.5 %`
sex_p1_Pace$upper_ci <- sex_p1_Pace$`97.5 %`

combined_p1_Pace = tidy(gee_Pace_1) %>% 
  cbind(as_tibble(confint.lm(gee_Pace_1))) %>% 
  filter(term == "anywk_new2")
combined_p1_Pace$name = "Pace"
combined_p1_Pace$title <- "DunedinPACE"
combined_p1_Pace$lower_ci <- combined_p1_Pace$`2.5 %`
combined_p1_Pace$upper_ci <- combined_p1_Pace$`97.5 %`


## PC GrimAge
sex_p1_pc_GrimAge = tidy(gee_pc_GrimAge_1_female) %>% 
  cbind(as_tibble(confint.lm(gee_pc_GrimAge_1_female))) %>% 
  filter(term == "anywk_new2")
sex_p1_pc_GrimAge = tidy(gee_pc_GrimAge_1_male) %>% 
  cbind(as_tibble(confint.lm(gee_pc_GrimAge_1_male))) %>% 
  filter(term == "anywk_new2") %>% 
  rbind(sex_p1_pc_GrimAge)
sex_p1_pc_GrimAge$name <- c("6_pc_GrimAge_Men", "4_pc_GrimAge_Women") 
sex_p1_pc_GrimAge$title <- "PC GrimAge"
sex_p1_pc_GrimAge$lower_ci <- sex_p1_pc_GrimAge$`2.5 %`
sex_p1_pc_GrimAge$upper_ci <- sex_p1_pc_GrimAge$`97.5 %`

combined_p1_pc_GrimAge = tidy(gee_pc_GrimAge_1) %>% 
  cbind(as_tibble(confint.lm(gee_pc_GrimAge_1))) %>% 
  filter(term == "anywk_new2")
combined_p1_pc_GrimAge$lower_ci <- combined_p1_pc_GrimAge$`2.5 %`
combined_p1_pc_GrimAge$upper_ci <- combined_p1_pc_GrimAge$`97.5 %`
combined_p1_pc_GrimAge$name = "pc_GrimAge"
combined_p1_pc_GrimAge$title = "PC GrimAge"


## PC Levine
sex_p1_pc_Levine = tidy(gee_pc_Levine_1_female) %>% 
  cbind(as_tibble(confint.lm(gee_pc_Levine_1_female))) %>% 
  filter(term == "anywk_new2")
sex_p1_pc_Levine = tidy(gee_pc_Levine_1_male) %>% 
  cbind(as_tibble(confint.lm(gee_pc_Levine_1_male))) %>% 
  filter(term == "anywk_new2") %>% 
  rbind(sex_p1_pc_Levine)
sex_p1_pc_Levine$name <- c("9_pc_PheoAge_Men", "7_pc_PhenoAge_Women") 
sex_p1_pc_Levine$title <- "PC PhenoAge"
sex_p1_pc_Levine$lower_ci <- sex_p1_pc_Levine$`2.5 %`
sex_p1_pc_Levine$upper_ci <- sex_p1_pc_Levine$`97.5 %`

combined_p1_pc_Levine = tidy(gee_pc_Levine_1) %>% 
  cbind(as_tibble(confint.lm(gee_pc_Levine_1))) %>% 
  filter(term == "anywk_new2")
combined_p1_pc_Levine$lower_ci <- combined_p1_pc_Levine$`2.5 %`
combined_p1_pc_Levine$upper_ci <- combined_p1_pc_Levine$`97.5 %`
combined_p1_pc_Levine$name = "pc_PhenoAge"
combined_p1_pc_Levine$title = "PC PhenoAge"


###### three main outcomes
sex_p1_main = rbind(sex_p1_Pace, sex_p1_pc_GrimAge, sex_p1_pc_Levine)
sex_p1_main$sex = c("Men", "Women", "Men", "Women", "Men", "Women")
sex_p1_main$sex <- factor(sex_p1_main$sex, levels = c("Women", "Men"))
sex_p1_main$name <- factor(sex_p1_main$name, levels=c("1_Pace_Women", "3_Pace_Men", "4_pc_GrimAge_Women", "6_pc_GrimAge_Men", "7_pc_PhenoAge_Women", "9_pc_PheoAge_Men"))

combined_p1 = rbind(combined_p1_Pace, combined_p1_pc_GrimAge, combined_p1_pc_Levine)
combined_p1$effect = c("combined", "combined", "combined")
combined_p1$effect <- factor(combined_p1$effect, levels = "combined")
combined_p1$name <- factor(combined_p1$name, levels=c("Pace", "pc_GrimAge", "pc_PhenoAge"))


# women and men
scaleFUN <- function(x) sprintf("%.1f", x)
sex_f1_main <- ggplot(sex_p1_main, aes(name, estimate, color = name)) + theme_bw() 
sex_f1_main <- sex_f1_main +        
  geom_point(size = 10, aes(color = name, shape = sex)) +
  geom_errorbar(aes(x = name, ymax = upper_ci, ymin = lower_ci), width = 0.2, size = 1.5) +
  labs(x = "", y = "Effect-sizes") +
  scale_x_discrete(labels = c("DunedinPACE", "", "PC GrimAge", "", "PC PhenoAge", "")) +
  scale_y_continuous(limits = c(-0.25, 0.45), breaks = seq(-0.2, 0.4, 0.2), labels=scaleFUN) +
  geom_hline(aes(yintercept = 0.0), color = "#666666", linewidth = 1, linetype="dashed") +
  theme(legend.title = element_blank(),
        legend.position = "top",
        legend.text = element_text(size = 25, color = "black"),
        axis.text = element_text(size = 25, color = "black"),
        panel.grid.major = element_line(linetype = "dotted", color = "grey", linewidth = 0.8),
        panel.grid.minor = element_blank(),
        panel.border = element_blank(),
        axis.line.y.left = element_line(colour='grey', linewidth = 1),
        axis.line.x.bottom = element_line(colour='grey', linewidth = 1),
        axis.ticks = element_line(colour='grey', linewidth = 1),
        axis.ticks.length=unit(.25, "cm"),
        axis.text.x = element_text(angle = 45, vjust = 0, hjust = 0)) + rremove("ylab") + rremove("xlab") +
        scale_color_manual(values = c("navyblue", "navyblue", "#990000", "#990000", "darkorange2", "darkorange2")) + guides(shape=guide_legend("sex"), color="none")
f1_panelB <- annotate_figure(sex_f1_main,
                top = text_grob("Panel B", size = 30, face = "bold"),
                left = text_grob("Effect-sizes", rot = 90, size = 30, face = "bold", vjust = -0.5), 
                bottom = text_grob("", size = 30, face = "bold", vjust = 1.5)) +
                theme(plot.margin = unit(c(1,1,1,1), "cm"))
```


```{r include=FALSE}
# combined women and men
f1_main <- ggplot(combined_p1, aes(name, estimate, color = name)) + theme_bw() 
f1_main <- f1_main +
        geom_point(size = 10, aes(color = name, shape=effect)) +
        scale_color_manual(values = c("navyblue", "#990000", "darkorange2")) +
        scale_shape_manual(values = c(18, 18, 18)) +
        guides(shape=guide_legend("effect"), color="none") +
