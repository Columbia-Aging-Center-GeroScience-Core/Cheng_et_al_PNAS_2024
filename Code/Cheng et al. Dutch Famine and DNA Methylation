########## Zhang ##########
### complete N=949 for clock_Zhang
DHWFS_data_Zhang <- DHWFS_data %>%
        drop_na(Zhang)
DHWFS_data_Zhang <- merge(DHWFS_data_Zhang, clocks_std_includingZhang, by = "hofnum")
DHWFS_data_Zhang_female <- merge(DHWFS_data_female, clocks_std_includingZhang, by = "hofnum")
DHWFS_data_Zhang_male <- merge(DHWFS_data_male, clocks_std_includingZhang, by = "hofnum")

DHWFS_data_Zhang_tc <- merge(DHWFS_data_tc, clocks_std_includingZhang, by = "hofnum")
DHWFS_data_Zhang_tc_female <- merge(DHWFS_data_tc_female, clocks_std_includingZhang, by = "hofnum")
DHWFS_data_Zhang_tc_male <- merge(DHWFS_data_tc_male, clocks_std_includingZhang, by = "hofnum")

DHWFS_data_Zhang_sib <- merge(DHWFS_data_sib, clocks_std_includingZhang, by = "hofnum") 
DHWFS_data_Zhang_sib <- DHWFS_data_Zhang_sib %>%
        filter(!hofnum==2271 & !hofnum==16720)
DHWFS_data_Zhang_sib_female <- merge(DHWFS_data_sib_female, clocks_std_includingZhang, by = "hofnum")
DHWFS_data_Zhang_sib_female <- DHWFS_data_Zhang_sib_female %>%
        filter(!hofnum==2271 & !hofnum==16720)
DHWFS_data_Zhang_sib_male <- merge(DHWFS_data_sib_male, clocks_std_includingZhang, by = "hofnum")
DHWFS_data_Zhang_sib_male <- DHWFS_data_Zhang_sib_male %>%
        filter(!hofnum==2271 & !hofnum==16720)


## Zhang ~ Famine exposure (full DHWFS: all, female, male, sex difference)
gee_Zhang_1 <- geeglm(formula = Zhang_std ~ anywk_new2 + Sex + Age + Agesq,
                      data = DHWFS_data_Zhang,
                      family = gaussian(link = "identity"),
                      id = idfam,
                      corstr  = "exchangeable")
gee_Zhang_1_female <- geeglm(formula = Zhang_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_Zhang_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Zhang_1_male <- geeglm(formula = Zhang_std ~ anywk_new2 + Age + Agesq,
                    data = DHWFS_data_Zhang_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Zhang_1_interact <- geeglm(formula = Zhang_std ~ anywk_new2*Sex + Age + Agesq,
                    data = DHWFS_data_Zhang,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## Zhang ~ Famine exposure (restricted to time controls: all, female, male, sex difference)
gee_Zhang_1_tc <- geeglm(formula = Zhang_std ~ anywk_new2 + Sex,
                     data = DHWFS_data_Zhang_tc,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Zhang_1_tc_female <- geeglm(formula = Zhang_std ~ anywk_new2,
                     data = DHWFS_data_Zhang_tc_female,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Zhang_1_tc_male <- geeglm(formula = Zhang_std ~ anywk_new2,
                     data = DHWFS_data_Zhang_tc_male,
                     family = gaussian(link = "identity"),
                     id = idfam,
                     corstr = "exchangeable")
gee_Zhang_1_tc_interact <- geeglm(formula = Zhang_std ~ anywk_new2*Sex,
                    data = DHWFS_data_Zhang_tc,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## Zhang ~ Famine exposure (siblings sample: all, female, male, sex difference)
fe_Zhang_1 <- plm(Zhang_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_Zhang_sib,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_Zhang_1_female <- plm(Zhang_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_Zhang_sib_female,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_Zhang_1_male <- plm(Zhang_std ~ anywk_new2 + Age + Agesq,
               data = DHWFS_data_Zhang_sib_male,
               index = c("idfam"), 
               model = "within", 
               effect = "individual")
fe_Zhang_1_interact <- plm(Zhang_std ~ anywk_new2*Sex + Age + Agesq,
                 data = DHWFS_data_Zhang_sib,
                 index = c("idfam"), 
                 model = "within", 
                 effect = "individual")

## Dose-Response (all, female, male, sex difference)
gee_Zhang_5.1 <- geeglm(formula = Zhang_std ~ prenatal_exp_10wks + Sex + Age + Agesq,
                       data = DHWFS_data_Zhang,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_Zhang_5.1_female <- geeglm(formula = Zhang_std ~ prenatal_exp_10wks + Age + Agesq,
                       data = DHWFS_data_Zhang_female,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_Zhang_5.1_male <- geeglm(formula = Zhang_std ~ prenatal_exp_10wks + Age + Agesq,
                       data = DHWFS_data_Zhang_male,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")
gee_Zhang_5.1_interact <- geeglm(formula = Zhang_std ~ prenatal_exp_10wks*Sex + Age + Agesq,
                       data = DHWFS_data_Zhang,
                       family = gaussian(link = "identity"),
                       id = idfam,
                       corstr = "exchangeable")

## Zhang ~ Exposure in a Specific Period (all, female, male)
gee_Zhang_2 <- geeglm(formula = Zhang_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Sex + Age + Agesq,
                      data = DHWFS_data_Zhang,
                      family = gaussian(link = "identity"),
                      id = idfam,
                      corstr = "exchangeable")
gee_Zhang_2_female <- geeglm(formula = Zhang_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq,
                    data = DHWFS_data_Zhang_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Zhang_2_male <- geeglm(formula = Zhang_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq,
                    data = DHWFS_data_Zhang_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
```
