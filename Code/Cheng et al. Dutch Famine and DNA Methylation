                       gee_pc_Hannum_1_tc_t, gee_pc_Hannum_1_tc_female_t, gee_pc_Hannum_1_tc_male_t, gee_pc_Hannum_1_tc_interact_t,
                       gee_Horvath2_1_tc_t, gee_Horvath2_1_tc_female_t, gee_Horvath2_1_tc_male_t, gee_Horvath2_1_tc_interact_t,
                       gee_pc_Horvath2_1_tc_t, gee_pc_Horvath2_1_tc_female_t, gee_pc_Horvath2_1_tc_male_t, gee_pc_Horvath2_1_tc_interact_t,
                       gee_GrimAge_1_tc_t, gee_GrimAge_1_tc_female_t, gee_GrimAge_1_tc_male_t, gee_GrimAge_1_tc_interact_t, 
                       gee_pc_GrimAge_1_tc_t, gee_pc_GrimAge_1_tc_female_t, gee_pc_GrimAge_1_tc_male_t, gee_pc_GrimAge_1_tc_interact_t,
                       gee_Levine_1_tc_t, gee_Levine_1_tc_female_t, gee_Levine_1_tc_male_t, gee_Levine_1_tc_interact_t,
                       gee_pc_Levine_1_tc_t, gee_pc_Levine_1_tc_female_t, gee_pc_Levine_1_tc_male_t, gee_pc_Levine_1_tc_interact_t,
                       gee_Zhang_1_tc_t, gee_Zhang_1_tc_female_t, gee_Zhang_1_tc_male_t, gee_Zhang_1_tc_interact_t), quiet = TRUE) %>% 
              as_gt() %>%
              gt::tab_style(
                style = gt::cell_text(weight = "bold"),
                locations = gt::cells_row_groups(groups = everything())
              ) %>% 
              gt::tab_header(title = "Effect of famine exposure")
