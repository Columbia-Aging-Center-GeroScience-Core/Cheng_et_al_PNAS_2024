x <- c("flextable", "gt", "gtsummary", "geepack", "ggpubr", "ggplot2", "magick", "Rcpp", "tableone", "readr", "dplyr", "tidyverse", "umx", "sjPlot", "plm", "fixest", "scales", "readstata13", "here", "haven")
lapply(x, require, character.only = TRUE)
