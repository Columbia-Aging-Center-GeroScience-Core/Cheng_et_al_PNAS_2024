## Sensitivity Analysis: cell-count-adjusted analysis
#### Models
```{r}
########## Pace ##########
## Pace ~ Famine exposure, adjusted for cell counts (all, female, male, sex difference)
gee_Pace_1.3 <- geeglm(formula = Pace_std ~ anywk_new2 + Sex + Age + Agesq +
                                            bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_1.3_female <- geeglm(formula = Pace_std ~ anywk_new2 + Age + Agesq +
                                        bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_1.3_male <- geeglm(formula = Pace_std ~ anywk_new2 + Age + Agesq +
                                      bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_1.3_interact <- geeglm(formula = Pace_std ~ anywk_new2*Sex + Age + Agesq +
                                          bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## Pace ~ dose response, adjusted for cell counts (all, female, male, sex difference)
gee_Pace_5.3 <- geeglm(formula = Pace_std ~ prenatal_exp_10wks + Sex + Age + Agesq +
                                            bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_5.3_female <- geeglm(formula = Pace_std ~ prenatal_exp_10wks + Age + Agesq +
                                                   bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_5.3_male <- geeglm(formula = Pace_std ~ prenatal_exp_10wks + Age + Agesq +
                                                 bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_5.3_interact <- geeglm(formula = Pace_std ~ prenatal_exp_10wks*Sex + Age + Agesq +
                                                     bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")

## Pace ~ gestational timing, adjusted for cell counts (all, female, male)
gee_Pace_2.3 <- geeglm(formula = Pace_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Sex + Age + Agesq + bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_2.3_female <- geeglm(formula = Pace_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq + bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data_female,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")
gee_Pace_2.3_male <- geeglm(formula = Pace_std ~ dwk_neg1vsall_new2 + dwk0vsall_new2 + dwk1vsall_new2 + dwk2vsall_new2 + dwk3vsall_new2 + dwk4vsall_new2 + Age + Agesq + bcell + cd4t + cd8t + gran + mono + nk,
                    data = DHWFS_data_male,
                    family = gaussian(link = "identity"),
                    id = idfam,
                    corstr = "exchangeable")


