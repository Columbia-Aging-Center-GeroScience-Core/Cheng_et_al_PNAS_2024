DHWFS_data %>%
        CreateTableOne(
        vars = select(DHWFS_data, 
                      Age, Sex, prenatal_exp_weeks,
                      Pace, pc_GrimAge, pc_Levine) %>% names(),
        strata = c("catthree3"),
        data = .,
        addOverall = TRUE) -> Table_S1_Panel1
